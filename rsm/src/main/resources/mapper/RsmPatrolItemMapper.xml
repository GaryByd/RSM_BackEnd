<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rc.mapper.RsmPatrolItemMapper">


    <select id="getPatrolItemByListId" resultType="com.rc.domain.entity.RsmPatrolItem">
        SELECT *
        FROM rsm_patrol_item
        WHERE patrol_list_id = #{checklistId}
    </select>
</mapper>
